@using SeyahatSitesi.Models.Sınıflar
@model List<Blog>

<div class="box1-left">
    @foreach (var X in Model)
    { 
        <div class="bot-img">
            <img src="@X.BlogImage" alt="" />
        </div>
        <div class="bot-desc1">
            <p>@X.Aciklama.Substring(0,50) </p>
        </div>
        <div class="clear"></div>
    }

</div>